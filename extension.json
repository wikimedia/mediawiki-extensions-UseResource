{
	"name": "UseResource",
	"version": "1.0.0",
	"author": "Brandon Fowler",
	"url": "https://www.mediawiki.org/wiki/Extension:UseResource",
	"descriptionmsg": "useresource-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\UseResource\\": "includes/"
	},
	"TestAutoloadNamespaces": {
		"MediaWiki\\Extension\\UseResource\\Tests\\": "tests/"
	},
	"Hooks": {
		"ParserFirstCallInit": "main",
		"OutputPageParserOutput": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\UseResource\\Hooks"
		}
	},
	"MessagesDirs": {
		"UseResource": [
			"i18n"
		]
	},
	"manifest_version": 2
}
